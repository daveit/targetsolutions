<div class="container">
  <div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8">
      <h3>Users</h3>

      <div class="column">
        <table class="table">
          <tbody>
          <% @users.each do |user| %>
              <tr>
                <%= render user %>
              </tr>
          <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
  <div class="col-md-2"></div>
</div>
