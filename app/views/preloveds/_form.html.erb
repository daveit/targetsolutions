<%= simple_form_for @preloved, html: {multipart: true} do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.input :name %>
      <%= f.input :model %>
      <%= f.association :make %>
      <%= f.input :price %>
      <%= f.input :details %>
      <%= f.input :image %>
      <%= f.input :active %>
    </div>

    <div class="form-actions">
      <%= f.button :submit, :class => "btn btn-small btn-success" %>
    </div>
<% end %>
