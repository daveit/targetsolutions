<%= simple_form_for @miscellaneou, html: { multipart: true } do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :model %>
    <%= f.association :make %>
    <%= f.input :details %>
    <%= f.input :qty %>
    <%= f.input :price %>
    <%= f.association :due %>
    <%= f.input :image %>
    <%= f.input :active %>
  </div>

  <div class="form-actions">
    <%= f.submit "Save Accessory", class: "btn btn-small btn-success" %>
  </div>
<% end %>
